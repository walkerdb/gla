    <dc:subject>Roberts, John Montgomery, 1807-1886</dc:subject>
    <dc:subject>Pioneers</dc:subject>
    <dc:subject>Roberts family</dc:subject>
    <dc:subject>Farm life</dc:subject>
    <dc:subject>Frontier and pioneer life</dc:subject>
